   <children>
      <AnchorPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label fx:id="itemProductName" layoutX="191.0" prefHeight="27.0" prefWidth="207.0" style="-fx-background-color: #09CDDA;" text="Product name here" />
            <Label fx:id="itemCategory" layoutX="191.0" layoutY="40.0" text="Category" />
            <Label fx:id="itemPrice" layoutX="191.0" layoutY="82.0" text="Price" />
            <ImageView fx:id="listingImage" fitHeight="121.0" fitWidth="179.0" layoutX="3.0" layoutY="7.0" onMouseClicked="#switchToListing" pickOnBounds="true" preserveRatio="true" />
